{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "jest": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
     "plugins": ["prettier"],
    "globals": {
        "user":"readonly",
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {        
        "eslint-disable-next-line no-undef": "off",
        "prettier/prettier": "error",
        "class-methods-use-this":"off",
        "consistent-return":"off",
        "no-param-reassing":"off"
    }
}